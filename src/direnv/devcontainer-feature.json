{
    "name": "Direnv",
    "id": "direnv",
    "version": "0.0.6",
    "description": "Installs and configures Direnv",
    "documentationURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/direnv/-/blob/v0.0.6/README.md",
    "licenseURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/direnv/-/blob/v0.0.6/LICENSE",
    "keywords": ["env"],
    "installsAfter": ["ghcr.io/geekstuff-dev/devcontainers-features/basics"],
    "customizations": {
        "vscode": {
            "extensions": [
                "mikestead.dotenv"
            ],
            "settings": {
                "[dotenv]": {
					"editor.detectIndentation": false,
					"editor.insertSpaces": true,
					"editor.tabSize": 4
				},
                "files.associations": {
                    "*.env.dist": "dotenv",
                    "*.env": "dotenv",
                    ".envrc": "dotenv"
                }
            }
        }
    }
}
