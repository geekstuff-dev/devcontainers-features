{
	"name": "Helm",
	"id": "helm",
	"version": "0.1.2",
	"description": "Installs helm",
	"documentationURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/helm/-/blob/v0.1.2/README.md",
	"licenseURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/helm/-/blob/v0.1.2/LICENSE",
	"options": {
        "version": {
            "type": "string",
            "default": "",
            "description": "The helm version"
        }
	},
	"customizations": {
		"vscode": {
			"extensions": [
				"Tim-Koehler.helm-intellisense"
			],
			"settings": {
				"files.associations": {
					"*.tpl": "helm-template"
				}
			}
		}
	}
}
