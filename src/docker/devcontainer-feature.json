{
    "name": "Docker",
    "id": "docker",
    "version": "0.0.4",
    "description": "Installs Docker clients and mounts Docker sock",
    "documentationURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/docker/-/blob/v0.0.4/README.md",
    "licenseURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/docker/-/blob/v0.0.4/LICENSE",
    "keywords": ["docker", "container"],
    "installsAfter": ["ghcr.io/geekstuff-dev/devcontainers-features/basics"],
    "mounts": [{
        "source": "/var/run/docker.sock",
        "target": "/var/run/docker.sock",
        "type": "bind"
    }]
}
