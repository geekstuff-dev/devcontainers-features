{
    "name": "Docker",
    "id": "docker",
    "version": "0.1.6",
    "description": "Installs Docker clients and mounts Docker sock",
    "documentationURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/docker/-/blob/v0.1.6/README.md",
    "licenseURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/docker/-/blob/v0.1.6/LICENSE",
    "installsAfter": ["ghcr.io/geekstuff-dev/devcontainers-features/basics"],
    "mounts": [{
        "source": "/var/run/docker.sock",
        "target": "/var/run/docker.sock",
        "type": "bind"
    }]
}
