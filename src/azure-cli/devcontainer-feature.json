{
    "name": "Azure CLI",
    "id": "azure-cli",
    "version": "0.1.0",
    "description": "Installs Azure CLI",
    "documentationURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/azure-cli/-/blob/v0.1.0/README.md",
    "licenseURL": "https://gitlab.com/geekstuff.dev/devcontainers/features/azure-cli/-/blob/v0.1.0/LICENSE",
    "installsAfter": [
		"ghcr.io/geekstuff-dev/devcontainers-features/basics",
		"ghcr.io/geekstuff-dev/devcontainers-features/docker"
	],
    "mounts": [{
        "source": "dev-${devcontainerId}-azure",
        "target": "/home/dev/.azure",
        "type": "volume"
    }],
    "containerEnv": {
        "DEV_LIB_AZURE": "/devcontainer/azure-cli"
    }
}
